{
  "expo": {
    "name": "Rune Of The Day",
    "slug": "RuneOfTheDay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "runeoftheday",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ravenwoodcreations.runeoftheday",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.ravenwoodcreations.runeoftheday",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "@bittingz/expo-widgets",
        {
          "android": {
            "widgetName": "RuneOfTheDay",
            "widgetLabel": "Rune of the Day",
            "widgetDescription": "Displays your daily rune with its meaning and deity",
            "widgetUpdateFrequency": "daily",
            "widgetPreviewImage": "./app/widgets/android/src/main/res/drawable/widget_preview.xml",
            "widgetProvider": "com.ravenwoodcreations.runeoftheday.RuneWidgetProvider",
            "widgetReceiver": "com.ravenwoodcreations.runeoftheday.RuneWidgetProvider",
            "sourceDir": "./app/widgets/android"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9c9afc04-4c7e-43c1-8df6-9d6da0246503"
      }
    }
  }
}
